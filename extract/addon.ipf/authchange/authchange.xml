<uiframe name="authchange" x="0" y="0" width="400" height="200" create="open">
    <frame title="{@st44}권한설정{/}" layout_gravity="center center"/>
    <draw drawtitlebar="true" drawframe="true"/>
    <option closebutton="false" hideable="true"/>
    <controls>
        <richtext name="t_title" rect="20 10 200 30" layout_gravity="left top" format="{@st45}이름:%s{/}" param1="Name"/>
