        <radiobutton name="survey_0" rect="20 100 90 20" caption="{@st49}없음" group="survey"/>
        <radiobutton name="survey_1" rect="110 100 90 20" caption="{@st49}작성" group="survey"/>
        <radiobutton name="survey_2" rect="200 100 90 20" caption="{@st49}삭제" group="survey"/>
