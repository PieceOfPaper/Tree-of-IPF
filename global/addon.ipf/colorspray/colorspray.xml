        <richtext name="t_remain" rect="0 0 90 24" margin="0 110 0 0" layout_gravity="center top" caption="{@st49}잔량{/}"/>
        <richtext name="t_brush" rect="0 0 90 24" margin="0 170 0 0" layout_gravity="center top" caption="{@st49}브러쉬{/}"/>
        <richtext name="hp_x" rect="0 0 150 22" margin="0 140 0 0" layout_gravity="center top" caption="{@st49}X %s{/}" format="{@st49}X %s{/}" param1="itemCnt"/>
        <button name="t_load" rect="0 0 80 24" margin="90 0 0 60" layout_gravity="center bottom" caption="{@st49}{a END_EDIT_SPRAY}그리기{/}{/}"/>
        <button name="t_end" rect="0 0 80 24" margin="0 0 0 60" layout_gravity="center bottom" caption="{@st49}{a SAVE_EDIT_SPRAY}저장{/}{/}"/>
        <button name="t_save" rect="0 0 80 24" margin="90 0 0 60" layout_gravity="center bottom" caption="{@st49}{a LOAD_EDIT_SPRAY}불러오기{/}{/}"/>
        <button name="t_close" rect="0 0 80 24" margin="90 0 0 30" layout_gravity="center bottom" LBtnUpScp="CLOSE_UI" caption="{@st49}닫기{/}"/>
        <picture name="colorList" rect="0 0 200 50" margin="80 80 0 0" layout_gravity="left top" LBtnUpScp="SELECT_SPRAY_COLOR" hittest="true" image="fullblack_200"/>
        <picture name="brush_1_bg" rect="0 0 32 32" margin="50 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_2_bg" rect="0 0 32 32" margin="86 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_4_bg" rect="0 0 32 32" margin="122 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_8_bg" rect="0 0 32 32" margin="158 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_16_bg" rect="0 0 32 32" margin="194 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_32_bg" rect="0 0 32 32" margin="230 195 0 0" layout_gravity="left top" image="fullwhite"/>
        <picture name="brush_1" rect="0 0 32 32" margin="67 210 0 0" layout_gravity="left top" LBtnUpScp="SELECT_SPRAY_BRUSH" hittest="true" image="spray_1"/>
        <picture name="brush_2" rect="0 0 32 32" margin="100 209 0 0" layout_gravity="left top" LBtnUpScp="SELECT_SPRAY_BRUSH" hittest="true" image="spray_2"/>
        <picture name="brush_4" rect="0 0 32 32" margin="135 208 0 0" layout_gravity="left top" LBtnUpScp="SELECT_SPRAY_BRUSH" hittest="true" image="spray_4"/>
        <picture name="brush_8" rect="0 0 32 32" margin="170 207 0 0" layout_gravity="left top" LBtnUpScp="SELECT_SPRAY_BRUSH" hittest="true" image="spray_8"/>
