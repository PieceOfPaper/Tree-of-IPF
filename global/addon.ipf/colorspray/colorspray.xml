<uiframe name="colorspray" x="-70" y="-320" width="320" height="330" create="open">
    <frame title="{@st43}컬러스프레이{/}" layout_gravity="right bottom" snapclient="true" snapframe="true"/>
    <skin frameskin="configskin" backgroundimage="mixer_back" layout_gravity="center center" rect="0 0 320 330"/>
    <draw drawtitlebar="false"/>
    <layer layerlevel="100"/>
    <option hideable="true" closebutton="true" useimageitem="true"/>
    <sound opensound="win_open" closesound="win_close"/>
    <script OpenScp="INIT_COLOR_SPRAY" CloseScp="CLOSE_COLORSPRAY"/>
