        <richtext name="title_time" parent="scoreGBox" rect="10 90 100 20" caption="시간" fontname="yellow_16"/>
        <richtext name="score_time" parent="scoreGBox" rect="-20 90 100 20" layout_gravity="right top" caption="99분 99초" fontname="white_16_ol" format="%s" param1="time" textalign="right center"/>
        <richtext name="title_total" parent="scoreGBox" rect="10 110 100 20" caption="총 점수" fontname="yellow_16"/>
        <richtext name="score_total" parent="scoreGBox" rect="-20 110 100 20" layout_gravity="right top" caption="38000 점" fontname="white_16_ol" format="%s" param1="total" textalign="right center"/>
        <slot name="giftitem" rect="20 -20 64 64" layout_gravity="left bottom"/>
        <richtext name="itemname" rect="90 -40 140 64" layout_gravity="left bottom" caption="향상된 신속의 물약" fixwidth="true" resizebytext="false"/>
        <timer name="addontimer" rect="0 0 10 10"/>
    </controls>
</uiframe>
