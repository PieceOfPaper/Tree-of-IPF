        <groupbox name="scoreGBox" rect="10 80 200 140" draw="false" hittest="false" scrollbar="false"/>
        <richtext name="title_kill" parent="scoreGBox" rect="10 10 100 20" caption="킬" fontname="yellow_16"/>
        <richtext name="score_kill" parent="scoreGBox" rect="-20 10 100 20" layout_gravity="right top" caption="3811" fontname="white_16_ol" format="%s" param1="kill" textalign="right center"/>
        <richtext name="title_overkill" parent="scoreGBox" rect="10 30 100 20" caption="오버킬" fontname="yellow_16"/>
        <richtext name="score_overkill" parent="scoreGBox" rect="-20 30 100 20" layout_gravity="right top" caption="3118" fontname="white_16_ol" format="%s" param1="overkill" textalign="right center"/>
        <richtext name="title_hit" parent="scoreGBox" rect="10 50 100 20" caption="피격" fontname="yellow_16"/>
        <richtext name="score_hit" parent="scoreGBox" rect="-20 50 100 20" layout_gravity="right top" caption="3811" fontname="white_16_ol" format="%s" param1="hit" textalign="right center"/>
        <richtext name="title_dead" parent="scoreGBox" rect="10 70 100 20" caption="사망" fontname="yellow_16"/>
